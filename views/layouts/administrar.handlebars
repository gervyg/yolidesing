<h1 class="text-alig-center py-5 px-5 my-5  ">LISTA DE PRESUPUESTOS </h1>




<div id="containerpresu" class="container-fluid mx-0 row col-sm-12 aling-left">
  <table id="tabla" class="table table-dark">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Nombre</th>
            <th scope="col">Rut</th>
            <th scope="col">Fecha de Solicitud</th>
            <th scope="col">Fecha de Validez</th>
            <th scope="col">Ver Detalle</th>            
            <th scope="col">Estado</th>
          </tr>
        </thead>
        <tbody>
         
          {{#each user}}
              <tr>
                  <th>{{this.id}}</th>  
                   <th>{{this.nombre}}</th>
                   <th>{{this.rut}}</th>               
                  <td>{{this.fecha_de_solicitud}}</td>
                  <td>{{this.fecha_de_validez}}</td>
                  <td><i class="fa-solid fa-magnifying-glass"></i></td>
                  <td>{{ this.estado}}</td>
                                   
              </tr>
          {{/each}}
        </tbody>
      </table>

  
</div>
















